<?xml version="1.0" encoding="UTF-8"?>
<window id="1115" type="dialog">
	<defaultcontrol always="true">90200</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>199</posx>
		<posy>210</posy>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1522</width>
			<height>660</height>
			<texture border="40">dialogs/default/bg.png</texture>
		</control>
		<control type="label" id="2">
			<description>header label</description>
			<posx>30</posx>
			<posy>32</posy>
			<width>1462</width>
			<height>40</height>
			<font>font15_title</font>
			<label>$LOCALIZE[42090]</label>
			<align>center</align>
			<textcolor>dialogheader</textcolor>
		</control>
		<control type="image">
			<posx>30</posx>
			<posy>86</posy>
			<width>1140</width>
			<height>542</height>
			<texture border="5">dialogs/default/inner.png</texture>
		</control>
		<control type="grouplist" id="90200">
			<description>control area</description>
			<posx>31</posx>
			<posy>88</posy>
			<width>1138</width>
			<height>540</height>
			<itemgap>0</itemgap>
			<pagecontrol>6</pagecontrol>
			<onup>90200</onup>
			<ondown>90200</ondown>
			<onleft>noop</onleft>
			<onright>9001</onright>
			
			<!-- Fade OSD when Paused -->
			<control type="button" id="90201">
                <width>1138</width>
                <label>$LOCALIZE[42091]</label>
                <label2>[COLOR labelheader]$VAR[OSDFadeLabel][/COLOR]</label2>
                <onclick>$VAR[OSDFadeSetting]</onclick>
            </control>
			
			<!-- Fade Delay -->
            <control type="button" id="90202">
                <width>1138</width>
                <label>$LOCALIZE[42095]</label>
                <label2>[COLOR labelheader]$VAR[OSDFadeTimeLabel][/COLOR]</label2>
                <onclick>$VAR[OSDFadeTimeSetting]</onclick>
				<enable>!IsEmpty(Skin.String(OSDFade))</enable>
            </control>
			
			<!-- Default OSD Button -->
			<control type="button" id="90203">
                <width>1138</width>
                <label>$LOCALIZE[42108]</label>
                <label2>[COLOR labelheader]$VAR[DefaultOSDButtonLabel][/COLOR]</label2>
                <onclick>$VAR[DefaultOSDButtonSetting]</onclick>
            </control>
			
			<!-- Show Large Pause Symbol When Paused -->
			<control type="radiobutton" id="90204">
                <width>1138</width>
                <label>$LOCALIZE[42117]</label>
                <onclick>Skin.ToggleSetting(DisablePauseSymbol)</onclick>
				<selected>!Skin.HasSetting(DisablePauseSymbol)</selected>
            </control>
			
		</control>
		
		<!-- Scrollbar -->
		<control type="scrollbar" id="6">
			<posx>1170</posx>
			<posy>88</posy>
			<width>8</width>
			<height>540</height>
			<texturesliderbackground>-</texturesliderbackground>
			<texturesliderbar colordiffuse="55ffffff" border="1">scrollbar/texturesliderbar2.png</texturesliderbar>
			<texturesliderbarfocus border="1">scrollbar/texturesliderbar2.png</texturesliderbarfocus>
			<onleft>90200</onleft>
			<onright>9001</onright>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
		</control>
		
		<!-- Right Group -->
		<control type="group">
			<posx>1182</posx>
			<posy>88</posy>
