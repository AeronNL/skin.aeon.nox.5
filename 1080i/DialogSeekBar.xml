<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>1</defaultcontrol>
	<visible>[Window.IsActive(VideoOSD) | Player.Seeking | Player.DisplayAfterSeek | Player.Caching] + Window.IsActive(FullScreenVideo)</visible>
	<zorder>1</zorder>
	<controls>
		<control type="group">
			<include>Animation_OpenCloseFade</include>
            <include>Animation_FadedByOSDSettings</include>
			<include>OSD_Player_Banner</include>
			<include>OSD_Progress_Bar2</include> 
			<include>MediaInfo2</include>
			
			
		</control>
		
		<control type="grouplist" id="2001">
		<animation effect="fade" start="0" end="100" time="200">Visible</animation>
        <animation effect="fade" start="100" end="0" time="200">Hidden</animation>
		<include>Animation_OpenCloseFade</include>
            <include>Animation_FadedByOSDSettings</include>
		
                <posx>298</posx>
                <posy>975</posy>
                <width>1577</width>
                <itemgap>0</itemgap>
                <orientation>horizontal</orientation>
                <onleft>709</onleft>
                <onright>11</onright>
                <onup>noop</onup>
                <ondown>noop</ondown>
				<visible>!Window.IsActive(VideoOSD) + [Player.Seeking | Player.DisplayAfterSeek | Player.Caching] + Window.IsActive(FullScreenVideo)</visible>
				
				<!-- OSD Playlist Button -->
                <control type="image">
                    <description>Playlists</description>
                    <height>100</height>
                    <width>100</width>
					<texture>osd/buttons/osdplaylistnf.png</texture>
                </control>  
								
				<!-- OSD Bookmarks Button -->
                <control type="image">
                    <description>Bookmarks</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdbookmarksnf.png</texture>
                </control>
								
				<!-- OSD Subtitles Button -->
                <control type="image">
                    <description>OSD Subtitles</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdsubtitlesnf.png</texture>
                </control>
				
				<!-- OSD Previous Button -->
                <control type="image">
                    <description>Previous</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdprevtracknf.png</texture>
                </control>
				
				<!-- OSD Rewind Button -->
                <control type="image">
                    <description>Rewind</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdrewindnf.png</texture>
                </control>
				
				<!-- OSD Stop Button -->
                <control type="image">
                    <description>Stop</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdstopnf.png</texture>
                </control>
				
				<!-- OSD Play/Pause Button -->
                <control type="image">
                    <description>Play</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdpausenf.png</texture>
                </control>
				
				<!-- OSD Fast Forward Button -->
                <control type="image">
                    <description>Forward</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdforwardnf.png</texture>
                </control>
				
				<!-- OSD Next Button -->
                <control type="image">
                    <description>Next</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdnexttracknf.png</texture>
                </control>
				
