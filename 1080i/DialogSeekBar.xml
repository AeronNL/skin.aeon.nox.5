				<!-- OSD Audio Settings -->
                <control type="image">
                    <description>Audio Settings</description>
                    <height>100</height>
                    <width>100</width>
                    <texture>osd/buttons/osdaudionf.png</texture>
                </control>
				
				<!-- OSD Options -->
				<control type="image">
					<texture>osd/buttons/OSDSkinNF.png</texture>
					<width>100</width>
					<height>100</height>
				</control>
