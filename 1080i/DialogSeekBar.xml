<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>1</defaultcontrol>
	<visible>[Player.Seeking | Player.DisplayAfterSeek | Player.Caching] + Window.IsActive(FullScreenVideo) + !Player.Forwarding</visible>
	<controls>
		<control type="group">
			<posy>1014</posy>
			<control type="image">
				<posx>0</posx>
				<posy>4</posy>
				<width>1920</width>
				<height>70</height>
				<texture border="1,32,1,1">bottombar.png</texture>
			</control>
			<control type="progress">
				<posx>0</posx>
				<posy>27</posy>
				<width>1920</width>
				<height>38</height>
				<texturebg border="2">osd/texturebg.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture border="2">osd/midtexture_cache.png</midtexture>
				<righttexture>-</righttexture>
				<overlaytexture>-</overlaytexture>
				<info>Player.ProgressCache</info>
			</control>
			<control type="progress">
				<posx>0</posx>
				<posy>28</posy>
				<width>1920</width>
				<height>35</height>
				<texturebg border="1">osd/texturebg_cache.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture border="2">osd/midtexture.png</midtexture>
				<righttexture>-</righttexture>
				<overlaytexture>-</overlaytexture>
				<info>Player.Progress</info>
				<colordiffuse>AAffffff</colordiffuse>
			</control>
			<control type="label">
				<posy>24</posy>
				<width>830</width>
				<height>90</height>
				<font>font14_title</font>
				<textoffsetx>15</textoffsetx>
				<textcolor>grey2</textcolor>
				<shadowcolor>shadow</shadowcolor>
				<align>left</align>
				<aligny>top</aligny>
				<textoffsety>-2</textoffsety>
				<label>$INFO[Player.Title]</label>
				<scroll>true</scroll>
			</control>
			<control type="label">
				<posy>24</posy>
				<posx>0</posx>
				<width>1920</width>
				<height>90</height>
				<font>font14_title</font>
				<textoffsetx>15</textoffsetx>
				<textcolor>grey2</textcolor>
				<shadowcolor>shadow</shadowcolor>
				<align>center</align>
				<aligny>top</aligny>
				<textoffsety>-2</textoffsety>
				<label>$VAR[SeekLabelVar]</label>
			</control>
			<control type="label">
				<posy>24</posy>
				<posx>1920</posx>
				<width>900</width>
				<height>90</height>
				<font>font14_title</font>
				<textoffsetx>15</textoffsetx>
				<textcolor>grey2</textcolor>
				<shadowcolor>shadow</shadowcolor>
				<align>right</align>
				<aligny>top</aligny>
				<textoffsety>-2</textoffsety>
				<label>$VAR[OSDRightLabelVar]</label>
			</control>
			<control type="slider" id="401">
				<description>Seek Slider</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>1920</width>
				<height>12</height>
				<texturesliderbar>seekslider2.png</texturesliderbar>
				<textureslidernib>osd_slider_nib.png</textureslidernib>
				<textureslidernibfocus>osd_slider_nib.png</textureslidernibfocus>
				<visible>Player.Seeking</visible>
			</control>
		</control>
		
		
	</controls>
</window>