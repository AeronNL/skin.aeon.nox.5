<?xml version="1.0" encoding="UTF-8"?>
<includes>
	
	<variable name="Homemenu_OrientationVar">
        <value condition="!Skin.HasSetting(Homemenu.Vertical)">$LOCALIZE[42184]</value>
        <value>$LOCALIZE[42183]</value>
    </variable>
	
	<variable name="middlemainmenuVar">
        <value condition="!Skin.HasSetting(middlemainmenu)">$LOCALIZE[42079]</value>
        <value>$LOCALIZE[42080]</value>
    </variable>
	
    <variable name="HorizontalListNavVar">
        <value condition="Skin.HasSetting(HorizontalListNav)">$LOCALIZE[31005]</value>
        <value>$LOCALIZE[571]</value>
    </variable>
	
	<variable name="MainHorizontalListNavVar">
        <value condition="Skin.HasSetting(MainHorizontalListNav)">$LOCALIZE[31005]</value>
        <value>$LOCALIZE[571]</value>
    </variable>
	
	<variable name="Homemenu_InfobarPositionVar">
        <value condition="Skin.HasSetting(Homemenu.LowerInfoBar)">$LOCALIZE[42079]</value>
        <value>$LOCALIZE[42189]</value>
    </variable>
	
	<!-- Number of Items on Horizontal Menu -->
    <variable name="MainMenuLayoutVar">
        <value condition="StringCompare(Skin.String(MainMenu.Layout),1 Item)">$LOCALIZE[42185]</value>
		<value condition="StringCompare(Skin.String(MainMenu.Layout),5 Items)">$LOCALIZE[42187]</value>
		<value>$LOCALIZE[42186]</value>
    </variable>
	
	<!-- Number of Items on Horizontal Menu Setting -->
	<variable name="MainMenuLayoutSettingVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Layout))">Skin.SetString(MainMenu.Layout,5 Items)</value>
        <value condition="StringCompare(Skin.String(MainMenu.Layout),5 Items)">Skin.SetString(MainMenu.Layout,1 Item)</value>
        <value condition="StringCompare(Skin.String(MainMenu.Layout),1 Item)">Skin.Reset(MainMenu.Layout)</value>
    </variable>
	
	<!-- Search Options Label -->
    <variable name="SearchLabelVar">
        <value condition="StringCompare(Skin.String(MainMenu.Search),Disabled)">$LOCALIZE[42093]</value> <!-- STRING: Disabled -->
		<value>$LOCALIZE[42209]</value> <!-- STRING: Homemenu -->
    </variable>
	
	<!-- Search Options Setting -->
	<variable name="SearchSettingVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Search))">Skin.SetString(MainMenu.Search,Disabled)</value>
        <value condition="StringCompare(Skin.String(MainMenu.Search),Disabled)">Skin.Reset(MainMenu.Search)</value>
    </variable>
	
	<!-- Home Menu Graphic Label -->
    <variable name="Homemenu_BarBackgroundLabelVar">
        <value condition="StringCompare(Skin.String(MainMenu.Graphic),Black Leaf)">$LOCALIZE[42202]</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Glass)">$LOCALIZE[42056]</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Mesh)">$LOCALIZE[42203]</value>
		<value>$LOCALIZE[31390]</value>
    </variable>
	
	<!-- Home Menu Graphic Setting -->
	<variable name="Homemenu_BarBackgroundSettingVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Graphic))">Skin.SetString(MainMenu.Graphic,Black Leaf)</value>
        <value condition="StringCompare(Skin.String(MainMenu.Graphic),Black Leaf)">Skin.SetString(MainMenu.Graphic,Glass)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Glass)">Skin.SetString(MainMenu.Graphic,Mesh)</value>
        <value condition="StringCompare(Skin.String(MainMenu.Graphic),Mesh)">Skin.Reset(MainMenu.Graphic)</value>
    </variable>
	
	<!-- Hack to Allow the Number 4 to be Displayed in the Watched Status Indicator Settings without being Localized -->
	<variable name="TVSample1Var">
		<value>4</value>
	</variable>
	
	<!-- Small Plot Setting Label -->
	<variable name="SettingSmallPlotLabelVar">
		<value condition="!Skin.HasSetting(General.SmallPlotText)">$LOCALIZE[31390]</value> <!-- STRING: Skin default -->
		<value condition="Skin.HasSetting(General.SmallPlotText)">$LOCALIZE[1438]</value> <!-- STRING: Small -->
	</variable>

	<!-- On Deck TV Icon Type -->
	<variable name="OnDeckTVIconTypeSettingVar">
		<value condition="IsEmpty(Skin.String(OnDeck.TVIconType))">Skin.SetString(OnDeck.TVIconType,Glass)</value>
		<value condition="StringCompare(Skin.String(OnDeck.TVIconType),Glass)">Skin.Reset(OnDeck.TVIconType)</value>
    </variable>
	
	<!-- Label for On Deck TV Icon Type -->
	<variable name="OnDeckTVIconTypeLabelVar">
		<value condition="IsEmpty(Skin.String(OnDeck.TVIconType))">$LOCALIZE[42058]</value> <!-- STRING: Poster -->
        <value>$LOCALIZE[42056]</value> <!-- STRING: Glass -->
    </variable>
	
	<!-- On Deck Music Icon Type -->
	<variable name="OnDeckMusicIconTypeSettingVar">
		<value condition="IsEmpty(Skin.String(OnDeck.MusicIconType))">Skin.SetString(OnDeck.MusicIconType,Glass)</value>
